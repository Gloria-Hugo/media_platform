* {
  padding: 0;
  margin: 0;
}
html {
  transform-origin: 50% 50% 0;
  width: 100%;
}
body {
  width: 100%;
  overflow-x: hidden;
}
div {
  box-sizing: border-box;
}
a {
  text-decoration: none;
  color: #333;
}
a:hover,
a:visited,
a:link,
a:active {
  color: #333;
}
ul,
ol {
  list-style: none;
}
.button {
  border: 0;
  background-color: none;
  outline: none;
  -webkit-appearance: none;
}
input,
textarea {
  outline: none;
  border: none;
}
textarea {
  resize: none;
  overflow: auto;
}
.clearfix:after {
  /*伪元素是行内元素 正常浏览器清除浮动方法*/
  content: "";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.clearfix {
  *zoom: 1;
  /*ie6清除浮动的方式 *号只有IE6-IE7执行，其他浏览器不执行*/
}
.warpBox {
  width: 1400px;
  margin: 0 auto;
}
.mainDark {
  background-color: #1a1a1e;
}
.mainColor {
  background-color: #f5482c;
}
.flex-box {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.el-pagination {
  margin-top: 40px;
  margin-left: 30px;
  margin-bottom: 60px;
}
.el-main,
.el-footer {

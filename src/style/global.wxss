}
#userManagerBox .el-form-item {
  border: 1px solid #ccc;
  padding-left: 14px;
}
#userManagerBox #queryBTN {
  border: none;
  border-radius: 0;
  border-color: none;
}
.changeBar {
  box-sizing: border-box;
  padding: 0 30px;
  background-color: #fff;
  width: 100%;

}
.searchBox {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.searchBox .queryIpt {
  border: 1px solid #cccccc;
  padding: 8px 14px;
  width: 300px;
  box-sizing: border-box;
}
.searchBox .queryIpt span {
  color: #26272a;
  font-size: 14px;
  font-weight: 500;
  margin-right: 15px;
  text-align: center;
  vertical-align: middle;
}
.searchBox .queryIpt .placehoder {
  color: #949aa6;
  font-size: 14px;
}
.searchBox .queryIpt input {
  border: none;
}
.searchBox .querybtn {
  margin-left: 20px;
  font-size: 16px;
  width: 78px;
  height: 36px;
  line-height: 36px;
}
.margin-box {
  margin: 30px;
}
.centerBox {
  padding: 30px;
}
.centerBox .blocks {
  position: relative;
  align-items: flex-start;
  width: 380px;
  background: #ffffff;
  border-radius: 4px;
  padding: 30px 0;
  float: left;
  margin-right: 14px;
  margin-bottom: 20px;
}
.centerBox .blocks h5,
.centerBox .blocks p,
.centerBox .blocks .mini-box {
  margin: 0 15px;
}
.centerBox .blocks .delete {
  position: absolute;
  right: 10px;
  top: 10px;
  height: 32px;
  width: 32px;
  line-height: 32px;
  text-align: center;
  border: 1px solid #ccc;
  border-radius: 50%;
}
.centerBox .blocks h5 {
  margin-bottom: 14px;
  font-size: 16px;
  color: #222;
  font-weight: bold;
}
.centerBox .blocks p {
  color: #949494;
  font-size: 12px;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical !important;
  word-break: break-all;
  height: 30px;
  line-height: 1.3;
}
.centerBox .blocks .mini-box {
  margin-top: 10px;
  margin-bottom: 20px;
  margin-right: 0;
}
.centerBox .blocks .mini-box span {
  color: #222222;
  font-size: 14px;
  font-weight: 400;
}
.centerBox .blocks .mini-box .shangxian {
  color: #222222;
}
.centerBox .blocks .mini-box .fujian {
  color: #2c6ef5;
}
.centerBox .blocks .launch-status {
  border-top: 1px dashed #ccc;
  padding: 20px 15px;
  padding-bottom: 0;
}
.centerBox .blocks .launch-status span {
  color: #949494;
  font-size: 14px;
  font-weight: 400;
}
.centerBox .blocks .launch-status :last-child {
  font-size: 14px;
  font-weight: 500;
}
.centerBox .blocks .launch-status .status {
  color: #f5482d;
}
.centerBox .blocks .launch-status .status1 {
  color: #222222;
}
.centerBox .blocks .options-box {
  margin-top: 15px;
  padding-top: 20px;
  padding-left: 20px;
  padding-right: 20px;
  border-top: 1px dashed #ccc;
}
.centerBox .blocks .options-box .profits {
  margin-right: auto;
  height: 36px;
  line-height: 36px;
}
.centerBox .blocks .options-box .btns {
  width: 80px;
  height: 36px;
  line-height: 36px;
  margin-left: auto;
}
.centerBox .blocks .options-box .btns1 {
  width: 80px;
  height: 36px;
  line-height: 36px;
  border: 1px solid #f5482c;
  color: #f5482c;
  text-align: center;
  margin-left: auto;
}
.centerBox .blocks .options-box .border-yellow {
  border: 1px solid #f5482c;
  border-radius: 2px;
  color: #f5482c;
  background-color: #fff;
}
.centerBox .blocks .options-box .border-grey {
  border: 1px solid#949494;
  border-radius: 2px;
  color: #949494;
  background-color: #fff;
}
.centerBox .blocks .options-box .btns-text {
  color: #949494;
  font-weight: 500;
  text-align: right;
  font-size: 14px;
  height: 36px;
  line-height: 36px;
}
.centerBox .blocks:hover {
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
}
#needs-details {
  margin: 30px;
  height: 100%;
}
#needs-details .sideBox {
  width: 780px;
  height: 90%;
  padding: 20px 40px;
  background: #ffffff;
}
#needs-details:first-child {
  margin-right: 10px;
}
#needs-details .leftBox .topBox,
#needs-details .rightBox .topBox {
  border-bottom: 1px dashed #ccc;
  padding-bottom: 20px;
}
#needs-details .leftBox .topBox :first-child,
#needs-details .rightBox .topBox :first-child {
  font-size: 18px;
  font-weight: 500;
  color: #222222;
}
#needs-details .leftBox .topBox :last-child,
#needs-details .rightBox .topBox :last-child {
  font-size: 14px;
  font-weight: 500;
  color: #949494;
}
#needs-details .block-box .block-head {
  width: 100%;
  height: 68px;
  background: rgba(245, 72, 44, 0.1);
  margin-top: 30px;
}
#needs-details .block-box .block-head .number {
  width: 80%;
  font-size: 18px;
  font-weight: bold;
  color: #f5482d;
  line-height: 68px;
  padding-left: 30px;
}
#needs-details .block-box .block-head .status {
  width: 20%;
  background-color: #fddbd6;
  line-height: 68px;
  font-size: 18px;
  font-weight: bold;
  color: #f5482d;
  text-align: center;
}
#needs-details #progress-box {
  background: #fbfbfb;
}
#needs-details .option-btns {
  display: flex;
  flex-direction: row;
  margin-top: 20px;
}
#needs-details .option-btns div {
  margin-left: auto;
  align-items: flex-end;
  padding: 13px 25px;
  text-align: center;
  border: 1px solid #cccccc;
  font-size: 14px;
  font-weight: 500;
}
#needs-details .option-btns .status1 {
  color: #fff;
  border-color: transparent;
}
#needs-details .option-btns .status2 {
  color: #f5482c;
  border-color: #f5482c;
}
#needs-details .option-btns .status3 {
  text-align: center;
  border: 1px solid #949494;
  box-sizing: border-box;
}
#needs-details .option-btns .profits {
  border: none;
  margin-right: auto;
  margin-left: 0;
  padding-left: 0;
  font-weight: bold;
  color: #222;
  font-size: 14px;
}
#needs-details .option-btns .contact {
  border: 1px solid #949494;
  color: #949494;
}
#needs-details .option-btns .pad {
  border: none;
}
#needs-details .rightBox .scripts-name {
  color: #222;
  font-size: 22px;
  font-weight: bold;
  margin: 40px 0;
}
#needs-details .rightBox .more-details {
  word-break: break-all;
}
#needs-details .rightBox .subtitle {
  color: #949494;
  font-weight: bold;
  font-size: 16px;
  position: relative;
  margin: 30px 0;
}
#needs-details .rightBox .subtitle::after {
  content: "";
  position: absolute;
  display: block;
  height: 2px;
  background-color: #f5472c;
  width: 24px;
  top: 30px;
}
#needs-details .rightBox .context {
  color: #222222;
  font-size: 14px;
  font-weight: 500;
}
#needs-details .rightBox .links a {
  display: block;
  text-decoration: underline;
  color: #2c6ef5;
  font-size: 14px;
  font-weight: 500;
}
#needs-details .rightBox .links .update {
  color: #f5472c;
  font-weight: 500;
  font-size: 14px;
  display: inline-block;
  margin-left: 15px;
}
#needs-details .rightBox .foot-box {
  width: 95%;
  align-items: center;
  height: 80px;
  box-shadow: 0px -4px 12px 0px rgba(102, 93, 92, 0.12);
}
.queryBox {
  position: relative;
  align-items: center;
}
.queryBox .normal {
  height: 48px;
  width: 120px;
  line-height: 48px;
  display: inline-block;
  background-color: #06d58e;
  top: 0;
}

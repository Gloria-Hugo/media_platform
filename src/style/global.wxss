}
/* .has-gutter tr {
  background-color: red !important;
} */
.el-form-item {
  margin-bottom: 0 !important;
}
.el-aside {
  padding-top: 60px;
  box-sizing: border-box;
  height: 100%;
  background: #ffffff;
}
.el-header {
  padding: 0 40px;
  box-sizing: border-box;
  width: 100%;
  height: 63px;
  background: #ffffff;
  box-shadow: 0px 2px 4px 0px rgba(65, 43, 42, 0.1);
  margin-bottom: 4px;
}
#userManagerBox {
  background-color: #fff;
  height: 90%;
  padding: 40px 26px;
}
#userManagerBox .el-input__inner {
